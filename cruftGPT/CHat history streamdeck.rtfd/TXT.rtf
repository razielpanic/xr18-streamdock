\cf16 const\cf0  inPort = \cf19 parseInt\cf0 (\cf17 getArg\cf0 (\cf21 '-port'\cf0 , \cf21 '0'\cf0 ), \cf18 10\cf0 );\
\cf16 const\cf0  inPluginUUID = \cf17 getArg\cf0 (\cf21 '-pluginUUID'\cf0 , \cf21 ''\cf0 );\
