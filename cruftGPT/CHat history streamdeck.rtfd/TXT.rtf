\cf20 \cb15 // xr18fx-bridge.js\cf0 \
\cf20 // WebSocket <-> XR18 bridge using ONE UDP socket + osc-min\cf0 \
\cf20 //\cf0 \
\cf20 // Requirements in this plugin folder:\cf0 \
\cf20 //   npm install osc-min ws\cf0 \
\cf20 //\cf0 \
\cf20 // Behavior:\cf0 \
\cf20 //   - Listens for WebSocket messages from the plugin (fader/mute commands)\cf0 \
\cf20 //   - Sends OSC to the XR18 FX returns: /rtn/N/mix/fader, /rtn/N/mix/on\cf0 \
\cf20 //   - Uses ONE UDP socket (dgram) for both send and receive\cf0 \
\cf20 //   - Polls XR18 every second for fader + mute state of FX1..4\cf0 \
\cf20 //   - For every OSC reply, broadcasts state back to all plugin instances\cf0 \
\
\cf16 const\cf0  dgram = \cf19 require\cf0 (\cf21 'dgram'\cf0 );\
\cf16 const\cf0  osc   = \cf19 require\cf0 (\cf21 'osc-min'\cf0 );\
\cf16 const\cf0  \cf17 WebSocket\cf0  = \cf19 require\cf0 (\cf21 'ws'\cf0 );\
\
\cf20 // XR18 network settings\cf0 \
\cf16 const\cf0  \cf17 XR18\cf0 _IP   = \cf21 '192.168.1.37'\cf0 ;\
\cf16 const\cf0  \cf17 XR18\cf0 _PORT = \cf18 10024\cf0 ;\
\
\cf20 // Local UDP port for our OSC socket (send + receive on this port)\cf0 \
\cf16 const\cf0  \cf18 LOCAL_OSC_PORT\cf0  = \cf18 10025\cf0 ;\
\
\cf20 // WebSocket port between plugin and bridge\cf0 \
\cf16 const\cf0  \cf18 BRIDGE_PORT\cf0  = \cf18 18018\cf0 ;\
\
\cf20 // Create single UDP socket\cf0 \
\cf16 const\cf0  udp = dgram.\cf17 createSocket\cf0 (\cf21 'udp4'\cf0 );\
\
\cf20 // WebSocket server for plugin connections\cf0 \
\cf16 const\cf0  wss = \cf16 new\cf0  \cf17 WebSocket\cf0 .\cf17 Server\cf0 (\{ \cf18 port\cf0 : \cf18 BRIDGE_PORT\cf0  \});\
\
\cf20 // Helper: OSC address for XR18 FX return N (1..4)\cf0 \
\cf16 function\cf0  \cf17 fxPath\cf0 (fx, suffix) \{\
  \cf20 // XR18 FX returns: /rtn/1..4/mix/...\cf0 \
  \cf16 return\cf0  \cf21 `/rtn/$\{fx\}\cf0 /$\{suffix\}`;\
\}\
\
\cf20 // Helper: send an OSC message to the XR18 from our single UDP socket\cf0 \
\cf16 function\cf0  \cf17 sendOsc\cf0 (address, args) \{\
  \cf16 try\cf0  \{\
    \cf16 const\cf0  msg = \{\
      address,\
      \cf18 args\cf0 : args || []   \cf20 // [] for query (no-argument) messages\cf0 \
    \};\
    \cf16 const\cf0  buf = osc.\cf17 toBuffer\cf0 (msg);\
    udp.\cf17 send\cf0 (buf, \cf18 0\cf0 , buf.length, \cf17 XR18\cf0 _PORT, \cf17 XR18\cf0 _IP);\
  \} \cf16 catch\cf0  (e) \{\
    \cf18 console\cf0 .\cf17 log\cf0 (\cf21 'sendOsc error:'\cf0 , e.message || e);\
  \}\
\}\
\
\cf20 // Broadcast state back to all connected plugin clients\cf0 \
\cf16 function\cf0  \cf17 broadcastState\cf0 (state) \{\
  \cf16 const\cf0  payload = \cf17 JSON\cf0 .\cf17 stringify\cf0 (\{\
    \cf18 type\cf0 : \cf21 'state'\cf0 ,\
    \cf18 fx\cf0 : state.fx,\
    \cf18 kind\cf0 : state.kind,   \cf20 // 'fader' or 'mute'\cf0 \
    \cf18 value\cf0 : state.value, \cf20 // for fader: 0.0..1.0\cf0 \
    \cf18 muted\cf0 : state.muted  \cf20 // for mute: boolean\cf0 \
  \});\
  wss.clients.\cf17 forEach\cf0 ((client) => \{\
    \cf16 if\cf0  (client.readyState === \cf17 WebSocket\cf0 .OPEN) \{\
      client.\cf17 send\cf0 (payload);\
    \}\
  \});\
\}\
\
\cf20 // Poll a single FX return for its current fader and mute state\cf0 \
\cf16 function\cf0  \cf17 pollFx\cf0 (fx) \{\
  \cf20 // Query current value by sending address with no arguments\cf0 \
  \cf17 sendOsc\cf0 (\cf17 fxPath\cf0 (fx, \cf21 'mix/fader'\cf0 ));\
  \cf17 sendOsc\cf0 (\cf17 fxPath\cf0 (fx, \cf21 'mix/on'\cf0 ));\
\}\
\
\cf20 // Poll all four FX returns\cf0 \
\cf16 function\cf0  \cf17 pollAllFx\cf0 () \{\
  [\cf18 1\cf0 , \cf18 2\cf0 , \cf18 3\cf0 , \cf18 4\cf0 ].\cf17 forEach\cf0 (pollFx);\
\}\
\
\cf20 // Handle incoming OSC messages (could be a single message or a bundle)\cf0 \
\cf16 function\cf0  \cf17 handleOscPacket\cf0 (buf) \{\
  \cf16 let\cf0  msg;\
  \cf16 try\cf0  \{\
    msg = osc.\cf17 fromBuffer\cf0 (buf);\
  \} \cf16 catch\cf0  (e) \{\
    \cf18 console\cf0 .\cf17 log\cf0 (\cf21 'OSC decode error:'\cf0 , e.message || e);\
    \cf16 return\cf0 ;\
  \}\
\
  \cf16 if\cf0  (!msg) \cf16 return\cf0 ;\
\
  \cf16 if\cf0  (msg.oscType === \cf21 'bundle'\cf0  && \cf17 Array\cf0 .\cf17 isArray\cf0 (msg.elements)) \{\
    msg.elements.\cf17 forEach\cf0 ((el) => \{\
      \cf16 if\cf0  (el && el.oscType === \cf21 'message'\cf0 ) \{\
        \cf17 handleOscMessage\cf0 (el);\
      \}\
    \});\
  \} \cf16 else\cf0  \cf16 if\cf0  (msg.oscType === \cf21 'message'\cf0 ) \{\
    \cf17 handleOscMessage\cf0 (msg);\
  \}\
\}\
\
\cf20 // Handle a single decoded OSC message from XR18\cf0 \
\cf16 function\cf0  \cf17 handleOscMessage\cf0 (message) \{\
  \cf16 const\cf0  addr = message.address;\
  \cf16 const\cf0  args = message.args || [];\
\
  \cf20 // For debug: log everything coming from mixer\cf0 \
  \cf18 console\cf0 .\cf17 log\cf0 (\cf21 'OSC RAW:'\cf0 , addr, args.\cf17 map\cf0 (a => a && a.value));\
\
  \cf20 // Expect replies like:\cf0 \
  \cf20 //   /rtn/1/mix/fader <float>\cf0 \
  \cf20 //   /rtn/1/mix/on    <int 0|1>\cf0 \
  \cf16 const\cf0  m = addr.\cf17 match\cf0 (\cf21 /^\\/rtn\\/(\\d+)\\/mix\\/(fader|on)$/\cf0 );\
  \cf16 if\cf0  (!m) \cf16 return\cf0 ;\
\
  \cf16 const\cf0  fx = \cf19 parseInt\cf0 (m[\cf18 1\cf0 ], \cf18 10\cf0 );\
  \cf16 const\cf0  kind = m[\cf18 2\cf0 ];\
  \cf16 if\cf0  (\cf17 Number\cf0 .\cf19 isNaN\cf0 (fx) || fx < \cf18 1\cf0  || fx > \cf18 4\cf0 ) \cf16 return\cf0 ;\
\
  \cf16 if\cf0  (kind === \cf21 'fader'\cf0 ) \{\
    \cf16 if\cf0  (args.length === \cf18 0\cf0 ) \cf16 return\cf0 ;\
    \cf16 const\cf0  arg = args[\cf18 0\cf0 ];\
    \cf16 let\cf0  value = \cf17 Number\cf0 (arg.value);\
    \cf16 if\cf0  (!\cf17 Number\cf0 .\cf19 isFinite\cf0 (value)) \cf16 return\cf0 ;\
\
    \cf18 console\cf0 .\cf17 log\cf0 (\cf21 'OSC REPLY FADER:'\cf0 , addr, value);\
    \cf17 broadcastState\cf0 (\{\
      fx,\
      \cf18 kind\cf0 : \cf21 'fader'\cf0 ,\
      value\
    \});\
  \} \cf16 else\cf0  \cf16 if\cf0  (kind === \cf21 'on'\cf0 ) \{\
    \cf16 if\cf0  (args.length === \cf18 0\cf0 ) \cf16 return\cf0 ;\
    \cf16 const\cf0  raw = \cf17 Number\cf0 (args[\cf18 0\cf0 ].value);\
    \cf16 const\cf0  muted = raw !== \cf18 0\cf0 ;\
\
    \cf18 console\cf0 .\cf17 log\cf0 (\cf21 'OSC REPLY MUTE:'\cf0 , addr, raw);\
    \cf17 broadcastState\cf0 (\{\
      fx,\
      \cf18 kind\cf0 : \cf21 'mute'\cf0 ,\
      muted\
    \});\
  \}\
\}\
\
\cf20 // UDP socket events\cf0 \
udp.\cf17 on\cf0 (\cf21 'listening'\cf0 , () => \{\
  \cf16 const\cf0  addr = udp.\cf17 address\cf0 ();\
  \cf18 console\cf0 .\cf17 log\cf0 (\cf21 `OSC UDP listening on $\{addr.address\}\cf0 :$\{addr.port\}, target $\{XR18_IP\}:$\{XR18_PORT\}`);\
\
  \cf20 // Ask mixer to send remote updates (optional; polling still used for truth)\cf0 \
  \cf17 sendOsc\cf0 (\cf21 '/xremote'\cf0 );\
\
  \cf20 // Initial poll when OSC is ready\cf0 \
