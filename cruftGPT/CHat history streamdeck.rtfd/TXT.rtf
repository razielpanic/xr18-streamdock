  \cf19 setInterval\cf0 (pollAllFx, \cf18 1000\cf0 );\
\});\
\
udp.\cf17 on\cf0 (\cf21 'message'\cf0 , (msg \cf20 /*, rinfo */\cf0 ) => \{\
