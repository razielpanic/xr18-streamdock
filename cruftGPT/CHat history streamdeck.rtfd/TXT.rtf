\cf20 \cb15 // xr18fx.js\cf0 \
\cf20 //\cf0 \
\cf20 // Minimal XR18 FX return controller for FX1\'96FX4.\cf0 \
\cf20 // Uses OSC over UDP to talk directly to the mixer.\cf0 \
\cf20 //\cf0 \
\cf20 // Dependencies: `node-osc`\cf0 \
\cf20 //   npm install node-osc\cf0 \
\cf20 //\cf0 \
\cf20 // XR18 notes:\cf0 \
\cf20 //   - OSC port: 10024\cf0 \
\cf20 //   - FX returns: /fxrtn/01 .. /fxrtn/08   (we\'92ll use 01..04) 
\f12 \uc0\u9733 
\f11 \cf0 \
\cf20 //   - Fader: /fxrtn/NN/mix/fader           (float 0.0..1.0, +10dB range) 
\f12 \uc0\u9733 
\f11 \cf0 \
\cf20 //   - On/off: /fxrtn/NN/mix/on             (int 0=OFF, 1=ON) 
\f12 \uc0\u9733 
\f11 \cf0 \
\cf20 //   - Name:   /fxrtn/NN/config/name        (string, up to 12 chars) 
\f12 \uc0\u9733 
\f11 \cf0 \
\cf20 //\cf0 \
\cf20 // 
\f12 \uc0\u9733 
\f11  from X32/X-Air OSC docs.:contentReference[oaicite:5]\{index=5\}\cf0 \
\
\cf16 const\cf0  osc = \cf19 require\cf0 (\cf21 "node-osc"\cf0 );\
\
\cf16 function\cf0  \cf17 clamp\cf0 (value, min, max) \{\
  \cf16 return\cf0  \cf17 Math\cf0 .\cf17 min\cf0 (max, \cf17 Math\cf0 .\cf17 max\cf0 (min, value));\
\}\
\
\cf16 function\cf0  \cf17 fxPath\cf0 (fxNumber, suffix) \{\
  \cf20 // fxNumber: 1..8\cf0 \
  \cf16 const\cf0  nn = \cf17 String\cf0 (fxNumber).\cf17 padStart\cf0 (\cf18 2\cf0 , \cf21 "0"\cf0 );\
  \cf16 return\cf0  \cf21 `/fxrtn/$\{nn\}\cf0 /$\{suffix\}`;\
\}\
\
\cf16 class\cf0  \cf17 XR18FxController\cf0  \{\
  \cf20 /**\
   * \cf16 @param\cf0  \{\cf18 Object\cf0 \} \cf18 options\cf0 \
   * \cf16 @param\cf0  \{\cf18 string\cf0 \} options.host  XR18 IP address (e.g. "192.168.1.50")\
   * \cf16 @param\cf0  \{\cf18 number\cf0 \} options.port  XR18 OSC UDP port (default 10024)\
   * \cf16 @param\cf0  \{\cf18 number[]\cf0 \} options.fxChannels  Which FX returns to control, e.g. [1,2,3,4]\
   */\
  \cf17 constructor\cf0 (\{\
    host = \cf21 "192.168.1.50"\cf0 ,\
    port = \cf18 10024\cf0 ,\
    fxChannels = [\cf18 1\cf0 , \cf18 2\cf0 , \cf18 3\cf0 , \cf18 4\cf0 ],\
  \} = \{\}) \{\
    \cf18 this\cf0 .host = host;\
    \cf18 this\cf0 .port = port;\
    \cf18 this\cf0 .fxChannels = fxChannels.\cf17 slice\cf0 (\cf18 0\cf0 , \cf18 4\cf0 ); \cf20 // we only care about 4 for this device\cf0 \
\
    \cf18 this\cf0 .client = \cf16 new\cf0  osc.\cf17 Client\cf0 (\cf18 this\cf0 .host, \cf18 this\cf0 .port);\
\
    \cf20 // Local state cache, one entry per logical knob index 0\'963\cf0 \
    \cf18 this\cf0 .state = \cf18 this\cf0 .fxChannels.\cf17 map\cf0 ((fxNum) => (\{\
      fxNum,\
      \cf18 fader\cf0 : \cf18 0.75\cf0 ,   \cf20 // default (will be overridden when we implement polling)\cf0 \
      \cf18 on\cf0 : \cf18 1\cf0 ,         \cf20 // 1 = ON (unmuted), 0 = OFF (muted)\cf0 \
      \cf18 name\cf0 : \cf21 `FX$\{fxNum\}\cf0 `, \cf20 // will be replaced by /config/name later\cf0 \
    \}));\
  \}\
\
  \cf17 close\cf0 () \{\
    \cf16 if\cf0  (\cf18 this\cf0 .client) \{\
      \cf18 this\cf0 .client.\cf17 close\cf0 ();\
      \cf18 this\cf0 .client = \cf16 null\cf0 ;\
    \}\
  \}\
\
  \cf20 /**\
   * Returns snapshot of state for UI (safe to read).\
   */\cf0 \
  \cf17 getState\cf0 () \{\
    \cf20 // Return a deep-ish copy so callers don\'92t mutate our internals\cf0 \
    \cf16 return\cf0  \cf18 this\cf0 .state.\cf17 map\cf0 ((ch) => (\{ ...ch \}));\
  \}\
\
  \cf20 /**\
   * Map a knob index 0..3 to an internal channel object.\
   */\cf0 \
  \cf17 _channelForIndex\cf0 (knobIndex) \{\
    \cf16 if\cf0  (knobIndex < \cf18 0\cf0  || knobIndex >= \cf18 this\cf0 .state.length) \{\
      \cf16 throw\cf0  \cf16 new\cf0  \cf17 Error\cf0 (\cf21 `Invalid knobIndex $\{knobIndex\}\cf0  (expected 0..$\{\cf18 this\cf0 .state.length - \cf18 1\cf0 \})`);\
    \}\
    \cf16 return\cf0  \cf18 this\cf0 .state[knobIndex];\
  \}\
\
  \cf20 /**\
   * Set fader for a logical knob 0..3, value 0.0..1.0 (linear).\
   * This sends /fxrtn/NN/mix/fader to the XR18 and updates local state.\
   */\cf0 \
  \cf17 setFaderByIndex\cf0 (knobIndex, value) \{\
    \cf16 const\cf0  channel = \cf18 this\cf0 .\cf17 _channelForIndex\cf0 (knobIndex);\
    \cf16 const\cf0  v = \cf17 clamp\cf0 (value, \cf18 0.0\cf0 , \cf18 1.0\cf0 );\
\
    channel.fader = v;\
\
    \cf16 const\cf0  path = \cf17 fxPath\cf0 (channel.fxNum, \cf21 "mix/fader"\cf0 );\
    \cf18 this\cf0 .\cf17 _send\cf0 (path, v);\
  \}\
\
  \cf20 /**\
   * Adjust fader by a delta (e.g. one detent of a rotary encoder).\
   * delta is usually small (e.g. +/- 0.01).\
   */\cf0 \
  \cf17 bumpFaderByIndex\cf0 (knobIndex, delta) \{\
    \cf16 const\cf0  channel = \cf18 this\cf0 .\cf17 _channelForIndex\cf0 (knobIndex);\
    \cf16 const\cf0  newValue = \cf17 clamp\cf0 (channel.fader + delta, \cf18 0.0\cf0 , \cf18 1.0\cf0 );\
    \cf18 this\cf0 .\cf17 setFaderByIndex\cf0 (knobIndex, newValue);\
  \}\
\
  \cf20 /**\
   * Toggle ON/OFF (mute) for the FX return mapped to knobIndex.\
   * We treat "on = 1" as unmuted; "on = 0" as muted.\
   */\cf0 \
  \cf17 toggleMuteByIndex\cf0 (knobIndex) \{\
    \cf16 const\cf0  channel = \cf18 this\cf0 .\cf17 _channelForIndex\cf0 (knobIndex);\
    \cf16 const\cf0  nextOn = channel.on ? \cf18 0\cf0  : \cf18 1\cf0 ; \cf20 // flip\cf0 \
\
    channel.on = nextOn;\
\
    \cf16 const\cf0  path = \cf17 fxPath\cf0 (channel.fxNum, \cf21 "mix/on"\cf0 );\
    \cf18 this\cf0 .\cf17 _send\cf0 (path, nextOn);\
  \}\
\
  \cf20 /**\
   * (Optional, for later) Request the config/name from XR18 so the UI can\
   * show the mixer\'92s own FX channel name (up to 12 chars).\
   * This just sends the query; you need a listener on the OSC UDP socket\
   * to receive and parse replies, which we\'92ll add once Stream Dock wiring is in place.\
   */\cf0 \
  \cf17 queryNamesOnce\cf0 () \{\
    \cf18 this\cf0 .state.\cf17 forEach\cf0 ((channel) => \{\
      \cf16 const\cf0  path = \cf17 fxPath\cf0 (channel.fxNum, \cf21 "config/name"\cf0 );\
      \cf18 this\cf0 .\cf17 _send\cf0 (path, \cf21 ""\cf0 ); \cf20 // X-Air responds with current value when sent a "get" packet\cf0 \
    \});\
  \}\
\
  \cf20 /**\
   * Low-level send wrapper with basic protection.\
   */\cf0 \
  \cf17 _send\cf0 (path, ...args) \{\
    \cf16 if\cf0  (!\cf18 this\cf0 .client) \cf16 return\cf0 ;\
\
    \cf16 try\cf0  \{\
      \cf18 this\cf0 .client.\cf17 send\cf0 (path, ...args);\
    \} \cf16 catch\cf0  (err) \{\
      \cf20 // For now just log to stderr; Stream Dock logs will show this.\cf0 \
      \cf18 console\cf0 .\cf17 error\cf0 (\cf21 "XR18FxController send error:"\cf0 , path, args, err);\
    \}\
  \}\
\}\
\
\cf18 module\cf0 .exports = \{\
  \cf17 XR18FxController\cf0 ,\
\};\
